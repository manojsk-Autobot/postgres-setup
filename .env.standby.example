# PostgreSQL Standby Server Environment Variables
# Copy this file to .env.standby and update with your actual values
# DO NOT commit .env.standby to version control!

# Database Configuration (must match primary)
POSTGRES_PASSWORD=change_this_password

# Replication Configuration
REPLICATION_USER=replicator
REPLICATION_PASSWORD=change_this_replication_password

# Network Configuration
PRIMARY_IP=192.168.1.100
